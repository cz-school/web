<view class="order"><cu-custom vue-id="1" bgColor="bg-gradual-white" isBold="{{isBold}}" isBack="{{true}}" url="/pages/self/self" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">我买到的</view></cu-custom><view class="order-list"><block wx:for="{{orderlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order-box"><view class="order-particulars"><view class="order-particulars-img"><image src="{{item.shop_img}}"></image></view><view class="order-particulars-specific"><view class="order-particulars-name"><text>{{item.shop_name}}</text></view><view class="order-particulars-price"><text>{{"￥"+item.shop_price}}</text></view><view class="order-particulars-state"><text data-event-opts="{{[['tap',[['ck',['$0'],[[['orderlist','',index,'order_comment']]]]]]]}}" bindtap="__e">交易成功</text></view></view></view><view class="order-comment"><block wx:if="{{item.order_comment==null}}"><button data-event-opts="{{[['tap',[['btn',['$0'],[[['orderlist','',index,'id']]]]]]]}}" bindtap="__e">评论</button></block><block wx:if="{{item.order_comment!==null}}"><button data-event-opts="{{[['tap',[['examine',['$0'],[[['orderlist','',index,'order_comment']]]]]]]}}" bindtap="__e">查看评论</button></block></view><view class="content"><modal vue-id="{{'2-'+index}}" title="{{title}}" value="{{show}}" data-event-opts="{{[['^confirm',[['confirm']]],['^event',[['event']]],['^input',[['__set_model',['','show','$event',[]]]]]]}}" bind:confirm="__e" bind:event="__e" bind:input="__e" bind:__l="__l"></modal></view></view></block></view></view>