<view class="regist"><cu-custom vue-id="1" bgColor="bg-transparent" url="{{url}}" isBack="{{true}}" bind:__l="__l"></cu-custom><view class="registForm"><uni-card vue-id="2" bind:__l="__l" vue-slots="{{['default']}}"><form class="loginBox"><view class="formLogin"><view class="inputBox inputMargin"><image class="inputImage" src="../../static/login/username.png" mode></image><input class="loginInput usernameInput" type="text" value="{{username}}" placeholder="账号" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" bindinput="__e"/></view><view class="inputBox"><image class="inputImage" src="../../static/login/password.png" mode></image><input class="loginInput" type="text" value="{{password}}" placeholder="设置密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" bindinput="__e"/></view></view></form></uni-card><uni-card class="submitBox" vue-id="3" is-shadow="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><button class="submitBtn" form-type="submit" data-event-opts="{{[['tap',[['regist',['$event']]]]]}}" bindtap="__e">注册</button></uni-card></view></view>