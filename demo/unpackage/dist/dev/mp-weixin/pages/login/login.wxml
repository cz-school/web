<view class="login"><zolysoft-nav-bar vue-id="1" backgroundColor="transparent" color="black" shadow="{{false}}" border="{{false}}" right-text="注册" bind:__l="__l"></zolysoft-nav-bar><view class="formCard"><uni-card vue-id="2" bind:__l="__l" vue-slots="{{['default']}}"><form class="loginBox"><view class="formLogin"><view class="inputBox inputMargin"><image class="inputImage" src="../../static/login/username.png" mode></image><input class="loginInput usernameInput" type="text" value="{{username}}" placeholder="账号/电话号码" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" bindinput="__e"/></view><view class="inputBox"><image class="inputImage" src="../../static/login/password.png" mode></image><input class="loginInput" type="text" value="{{password}}" placeholder="密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" bindinput="__e"/></view></view></form></uni-card><uni-card class="submitBox" vue-id="3" is-shadow="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><button class="submitBtn" form-type="submit" data-event-opts="{{[['tap',[['Login',['$event']]]]]}}" bindtap="__e">登录</button></uni-card><text class="forget">忘记密码</text><view class="otherLogin"><view class="otherLogin-top"><image class="otherImage otherLeft" src="../../static/login/weixin.png" mode></image><image class="otherImage" src="../../static/login/QQ.png" mode data-event-opts="{{[['tap',[['qqLogin',['$event']]]]]}}" bindtap="__e"></image></view><view class="otherLogin-bottom"><text>登录即代表<text class="otherText">《用户协议》</text>和<text class="otherText">《隐私政策》</text></text></view></view></view></view>