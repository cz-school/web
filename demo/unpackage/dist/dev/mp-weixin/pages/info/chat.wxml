<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" url="{{url}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content" isBold="{{isBold}}">{{username}}</view></cu-custom><view class="cu-chat"><block wx:for="{{infoList}}" wx:for-item="item" wx:for-index="index"><view class="{{['cu-item',item.id==user_id?'self':'']}}"><block wx:if="{{item.id==user_id}}"><view class="main"><view class="content shadow"><text>{{item.message}}</text></view></view><view class="cu-avatar radius" style="{{'background-image:'+('url('+item.head_img+')')+';'}}"></view><view class="date">13:23</view></block><block wx:if="{{item.id!=user_id}}"><view class="cu-avatar radius" style="{{'background-image:'+('url('+item.head_img+')')+';'}}"></view><view class="main"><view class="content shadow"><text>{{item.message}}</text></view></view><view class="date">13:23</view></block></view></block></view><view class="cu-bar foot input" style="{{'bottom:'+(InputBottom+'px')+';'}}"><view class="action"><text class="cuIcon-sound text-grey"></text></view><input class="solid-bottom" adjust-position="{{false}}" focus="{{false}}" maxlength="300" cursor-spacing="10" data-event-opts="{{[['focus',[['InputFocus',['$event']]]],['blur',[['InputBlur',['$event']]]],['input',[['__set_model',['','info','$event',[]]]]]]}}" value="{{info}}" bindfocus="__e" bindblur="__e" bindinput="__e"/><view class="action"><text class="cuIcon-emojifill text-grey" name="text"></text></view><button data-event-opts="{{[['tap',[['sendInfo',['$event']]]]]}}" class="cu-btn bg-green shadow" bindtap="__e">发送</button></view></view>